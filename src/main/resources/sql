create table categories
(
    id   integer not null
        primary key,
    name TEXT    not null
);



create table dict_data
(
    id        integer not null
        primary key autoincrement,
    dict_code text    not null,
    code      text    not null,
    name      text    not null
);



create table items
(
    id            INTEGER
        primary key autoincrement,
    name          TEXT,
    price         REAL,
    purchase_date TEXT,
    remark        text,
    category_id   INTEGER,
    status        TEXT
);